{
  "profile": "tabular-data-package",
  "title": "ORDS Extra - 2025/07",
  "description": "Additional data to complement that of the Open Repair Alliance.",
  "homepage": "https://github.com/zenlan/ords-extra",
  "contributors": [
    {
      "title": "Monique Szpak",
      "role": "author"
    }
  ],
  "licenses": [
    {
      "name": "CC BY-NC-ND 4.0",
      "title": "Creative Commons Attribution-NonCommercial-NoDerivatives 4.0 International ",
      "path": "https://creativecommons.org/licenses/by-nc-nd/4.0"
    }
  ],
  "resources": [
    {
      "profile": "tabular-data-resource",
      "format": "CSV",
      "name": "ords_problem_en_[translation-tool].csv",
      "path": "https://github.com/zenlan/ords-extra/",
      "title": "ORDS Problem Text English Translations",
      "description": "Text strings translated by the [translation-tool] referenced by the filename, e.g. 'deepl'. See the README.md for details.",
      "schema": {
        "fields": [
          {
            "name": "id",
            "type": "string",
            "format": "default",
            "title": "ID",
            "description": "ORDS unique identifier.",
            "constraints": {
              "pattern": "^[a-z]+-[0-9]+$",
              "unique": "true"
            }
          },
          {
            "name": "language",
            "type": "string",
            "format": "default",
            "title": "Language Code",
            "description": "2 character ISO language code.",
            "constraints": {
              "pattern": "^[a-z]{2}$"
            }
          },
          {
            "name": "en",
            "type": "string",
            "format": "default",
            "title": "English Problem Text",
            "description": "English, free-text description of the repair attempt",
            "missingValues": [
              ""
            ]
          }
        ],
        "primaryKey": "id"
      }
    },
    {
      "profile": "tabular-data-resource",
      "format": "CSV",
      "name": "ords_product_en.csv",
      "path": "https://github.com/zenlan/ords-extra/",
      "title": "ORDS Product English Translations",
      "schema": {
        "fields": [
          {
            "name": "id",
            "type": "string",
            "format": "default",
            "title": "ID",
            "description": "ORDS unique identifier.",
            "constraints": {
              "pattern": "^[a-z]+-[0-9]+$",
              "unique": "true"
            }
          },
          {
            "name": "partner_category",
            "type": "string",
            "format": "default",
            "title": "Partner Category",
            "description": "Category provided by the data contributor.",
            "missingValues": [
              ""
            ]
          },
          {
            "name": "partner_product",
            "type": "string",
            "format": "default",
            "title": "Partner Product",
            "description": "Product type provided by the data contributor.",
            "missingValues": [
              ""
            ]
          },
          {
            "name": "language",
            "type": "string",
            "format": "default",
            "title": "Language Code",
            "description": "2 character ISO language code.",
            "constraints": {
              "pattern": "^[a-z]{2}$"
            }
          },
          {
            "name": "en",
            "type": "string",
            "format": "default",
            "title": "Problem Text",
            "description": "English, translation of the product type",
            "missingValues": [
              ""
            ]
          }
        ],
        "primaryKey": "id"
      }
    },
    {
      "profile": "tabular-data-resource",
      "format": "CSV",
      "name": "ords_unu_keys.csv",
      "path": "https://github.com/zenlan/ords-extra/",
      "title": "ORDS UNU Key Map",
      "schema": {
        "fields": [
          {
            "name": "id",
            "type": "string",
            "format": "default",
            "title": "ID",
            "description": "ORDS unique identifier.",
            "constraints": {
              "pattern": "^[a-z]+-[0-9]+$",
              "unique": "true"
            }
          },
          {
            "name": "product_category",
            "type": "string",
            "format": "default",
            "title": "ORDS Product Category.",
            "description": "Name of the ORDS product category."
          },
          {
            "name": "product",
            "type": "string",
            "format": "default",
            "title": "Product Name",
            "description": "Free text product name."
          },
          {
            "name": "unu_key",
            "type": "integer",
            "format": "default",
            "title": "UNU Key ID",
            "description": "UNU key unique identifier.",
            "constraints": {
              "pattern": "^[0-9]+$"
            }
          },
          {
            "name": "unu_str",
            "type": "string",
            "format": "default",
            "title": "UNU Key Title",
            "description": "UNU key text description."
          }
        ],
        "primaryKey": "id"
      }
    }
  ]
}